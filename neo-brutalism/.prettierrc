{
    "plugins": ["prettier-plugin-tailwindcss"],
    "pluginSearchDirs": false,
    "arrowParens": "always",
    "bracketSpacing": true,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "printWidth": 90,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "singleQuote": true,
    "semi": true,
    "tabWidth": 4,
    "trailingComma": "es5"
}
